<config>
    <modules>
        <Tejar_Sitemap>
            <version>1.6.0.0</version>
        </Tejar_Sitemap>
    </modules>
    <global>
        <models>
            <sitemap_resource>
				<rewrite>
					<cms_page>Tejar_Sitemap_Model_Resource_Cms_Page</cms_page>
				</rewrite>
            </sitemap_resource>
            <sitemap>
				<rewrite>
					<sitemap>Tejar_Sitemap_Model_Sitemap</sitemap>
				</rewrite>
			</sitemap>
        </models>
    </global>
</config>
